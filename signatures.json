        ".exe", ".ps1", ".vbs", ".bat", ".cmd",
        ".js", ".jse", ".vbe", ".wsf", ".wsh",
        ".scr", ".pif", ".com", ".dll", ".ocx"
    ],
    "benignStreams": [
        "Zone.Identifier",
        "encryptable",
        "SummaryInformation",
        "DocumentSummaryInformation",
        "{$I30",
        "OECustomProperty",
        "com.apple.quarantine",
        "AFP_AfpInfo",
        "AFP_Resource"
    ],
    "encodingPatterns": [
        "^[A-Za-z0-9+/=]{20,}$",
        "^[0-9A-Fa-f\\s]{20,}$",
        "powershell.*[\\-\\/][eE][nN][cC]",
        "frombase64string",
        "invoke[\\-\\_]expression",
        "iex\\s*\\(|invoke\\-expression",
